<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="container">
		<%= render 'layouts/navigation' %>
		<div id="header">
			<div id="wrapper">
				
				<%= yield %>
				
			</div>
		</div>
		<%= render 'layouts/footer' %>
	</div>
</body>
</html>
